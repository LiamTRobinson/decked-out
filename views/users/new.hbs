<main class="valign-wrapper">
	<div class="row">
		<form action="/users" method="POST" class="col s12 offset-l2 offset-m2">
			<div class="row">
				<div class="col s6 input-field">
					<i class="material-icons prefix hide-on-small-only">perm_identity</i>
					<label for="firstName">First Name: </label>
					<input type="text" name="firstName" class="grey-text text-darken-3 validate" required>
				</div>
				<div class="col 6 input-field">
					<i class="material-icons prefix hide-on-small-only">perm_identity</i>
					<label for="lastName">Last Name: </label>
					<input type="text" name="lastName" class="grey-text text-darken-3 validate" required>
				</div>
			</div>
			<div class="row">
				<div class="col s12 input-field">
					<i class="material-icons prefix hide-on-small-only">email</i>
					<label for="email">Email: </label>
					<input type="email" name="email" class="grey-text text-darken-3 validate" required>
				</div>
			</div>
			<div class="row">
				<div class="col s6 input-field">
					<i class="material-icons prefix hide-on-small-only">person_pin</i>
					<label for="userName">User Name: </label>
					<input type="text" name="userName" class="grey-text text-darken-3 validate" required>
				</div>
				<div class="col s6 input-field">
					<i class="material-icons prefix hide-on-small-only">lock</i>
					<label for="password">Password: </label>
					<input type="text" name="password" class="grey-text text-darken-3 validate" required>
				</div>	
			</div>
			<div>
				<input type="submit" value="Create Account" class="btn grey darken-3">
			</div>
		</form>
	</div>
</main>	
	

